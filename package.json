{
  "name": "weatherapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start -- --reset-cache",
    "web": "expo start --web",
    "build:web": "expo build:web",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pod-install": "npx pod-install",
    "reinstall": "rm -rf node_modules && rm yarn.lock && yarn install && npx pod-intall ios",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "doctoc": "doctoc --title='## Table of Contents' --github README.md",
    "predoctoc:check": "cp README.md v-README.md.orig && yarn run doctoc",
    "doctoc:check": "diff -q README.md v-README.md.orig",
    "postdoctoc:check": "mv v-README.md.orig README.md",
    "translate": "npx mady -d \"src/app/locales\"",
    "typedoc": "npx typedoc"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.9",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/netinfo": "^6.0.2",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.4",
    "@react-navigation/stack": "^6.0.11",
    "@types/react": "^17",
    "axios": "^0.24.0",
    "expo": "^42.0.4",
    "expo-asset": "~8.3.3",
    "expo-font": "~9.2.1",
    "expo-linear-gradient": "~9.2.0",
    "expo-localization": "~10.2.0",
    "expo-location": "~12.1.2",
    "expo-splash-screen": "~0.11.2",
    "mady": "^4.1.0",
    "moment-timezone": "^0.5.33",
    "react": "17.0.2",
    "react-native": "0.66.0",
    "react-native-reanimated": "^2.3.0-beta.2",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-svg": "^12.1.1",
    "react-native-unimodules": "^0.14.8",
    "react-native-web": "^0.17.5",
    "use-count-up": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^6.3",
    "@storybook/addon-knobs": "^6.3",
    "@storybook/addon-links": "^6.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.65.0",
    "@types/react-test-renderer": "^17.0.1",
    "@welldone-software/why-did-you-render": "^6.2.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "core-js": "3",
    "doctoc": "^2.1.0",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "mady-server": "^4.1.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-dom": "17.0.2",
    "react-native-storybook-loader": "^2.0.4",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "17.0.2",
    "typedoc": "^0.22.7",
    "typescript": "4.0.8"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/danieldaleva/weather-app.git"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.@(js|jsx|ts|tsx|mdx)",
      "outputFile": "./src/storybook/storyLoader.js"
    }
  }
}
